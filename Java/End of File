import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        
        int c = 0;
        while(scan.hasNext()){ 
        c++;
        System.out.println(c + " " + scan.nextLine());
        }
    }
}

